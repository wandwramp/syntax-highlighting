{
    "name": "wramp-syntax",
    "displayName": "WRAMP Syntax Highlighting",
    "description": "WRAMP Assembly syntax highlighting",
    "icon": "images/icon.png",
    "version": "1.1.0",
    "license": "SEE LICENSE in LICENSE",
    "publisher": "wanduow",
    "repository": {
	    "type": "git",
	    "url": "https://github.com/wandwramp/syntax-highlighting.git"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "wramp",
            "aliases": ["WRAMP Assembly", "wramp"],
            "extensions": [".s",".S"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "wramp",
            "scopeName": "source.wramp",
            "path": "./syntaxes/wramp.tmLanguage.json"
        }]
    }
}
